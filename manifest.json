{
  "name": "RSSFeed",
  "version": "0.0.4",
  "description": "To the moon!",
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "scripts": [
      "lib/jquery-3.4.1.min.js",
      "lib/lodash-core.js",
      "lib/http.js",
      "js/backgroundConstants.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_title": "RSSFeed"
  },
  "icons": {
		"16": "img/logo.png",
		"48": "img/logo.png",
		"128": "img/logo.png"
  },
  "permissions": [
		"tabs",
		"storage",
    "*://*/*"
  ]
}
